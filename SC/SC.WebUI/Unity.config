<?xml version="1.0" encoding="utf-8"?>
<unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
  <sectionExtension type="Microsoft.Practices.Unity.InterceptionExtension.Configuration.InterceptionConfigurationExtension, Microsoft.Practices.Unity.Interception.Configuration" />
  <typeAliases>
    <!-- Services -->
    <typeAlias alias="IUnitOfWork" type="SC.Repository.IUnitOfWork,SC.Repository" />
    <typeAlias alias="UnitOfWork" type="SC.Repository.UnitOfWork,SC.Repository" />
    <!--<typeAlias alias="IAuthenticationService" type="SC.Infrastructure.Security.IAuthenticationService, SC.Infrastructure" />
    <typeAlias alias="FormsAuthenticationService" type="SC.Infrastructure.Security.Providers.FormsAuthenticationService, SC.Infrastructure" />-->

    <!--Begin-->

    <!--Demo-->
    <typeAlias alias="IDemoService" type="SC.Business.Interface.IDemoService,SC.Business.Interface" />
    <typeAlias alias="DemoService" type="SC.Business.Implement.DemoService,SC.Business.Implement" />

    <!--Menu-->
    <typeAlias alias="IMenuService" type="SC.Business.Interface.IMenuService,SC.Business.Interface" />
    <typeAlias alias="MenuService" type="SC.Business.Implement.MenuService,SC.Business.Implement" />

    <!--User-->
    <typeAlias alias="IUserService" type="SC.Business.Interface.IUserService,SC.Business.Interface" />
    <typeAlias alias="UserService" type="SC.Business.Implement.UserService,SC.Business.Implement" />

    <!--Role-->
    <typeAlias alias="IRoleService" type="SC.Business.Interface.IRoleService,SC.Business.Interface" />
    <typeAlias alias="RoleService" type="SC.Business.Implement.RoleService,SC.Business.Implement" />
    <!--End-->
  </typeAliases>
  <container>
    <extension type="Interception" />
    <register type="IUnitOfWork" mapTo="UnitOfWork" />
    <!--<register type="IAuthenticationService" mapTo="FormsAuthenticationService"/>-->

    <!--Begin-->
    <register type="IDemoService" mapTo="DemoService" />
    <register type="IMenuService" mapTo="MenuService" />
    <register type="IUserService" mapTo="UserService" />
    <register type="IRoleService" mapTo="RoleService" />
    <!--End-->
  </container>
</unity>
